{
  "name": "viralix-backend",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "worker": "tsx src/worker.ts",
    "dev:all": "npm-run-all -p dev worker",
    "build": "tsc -p tsconfig.json",
    "postinstall": "prisma generate",
    "start": "node dist/index.js",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "seed": "tsx --tsconfig tsconfig.seed.json prisma/seed.ts",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.58.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "qs": "^6.14.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.4.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tsx": "^4.20.5",
    "ts-jest": "^29.2.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  }
}
